<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Inicio</th>
      <th scope="col">Termino</th>
      <th scope="col">Persona</th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
    <%= for block_user <- @blocks_users do %>
      <tr>
        <td><%= Timex.format!(block_user.block.start_at, "%d de %b del %Y - %H:%M", :strftime) %></td>
        <td><%= Timex.format!(block_user.block.end_at, "%d de %b del %Y - %H:%M", :strftime) %></td>
        <td><%= block_user.user.email %></td>
        <td>
          <span><%= link "Cambiar", to: Routes.block_path(@conn, :show, block_user.block) %></span>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
